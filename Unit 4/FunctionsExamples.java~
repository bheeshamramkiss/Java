public class FunctionsExamples {
    
    
    public static void main(String[] args) { 
       double x, y;
       
       x = -3.6;
       
       y = Math.abs(x);
       
       System.out.println(y);
       
       y = Math.pow(x, 2);
       
       System.out.println(y);
       
       y = Math.round(y);
       
       System.out.println(y);
    }

    
}
