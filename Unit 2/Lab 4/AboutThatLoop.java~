public class AboutThatLoop
{
    public static void main (String[] args)
    {
        System.out.println ("Because you know,");
        
        for ( int i = 1; i <=5 ; i++)
        {
            
            for ( int j = 1 ; j <= 3 ; j++)
            {
                System.out. print ("I'm all about that loop,");
                
            }
            
            System.out. print (" no trouble");
            
            System.out.println () ;
            
        }
        
        System.out.println ("Yeah it's pretty clear, I can do all that looping now..");
        
    }
}